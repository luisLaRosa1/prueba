    "husky": "^7.0.4",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-helpful-reporter": "^0.3.4",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
