{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "start:ssl": "ng serve --ssl",
    "testnw": "ng test --no-watch --browsers=ChromeHeadless --code-coverage",
    "test:prepush": "ng test --reporters spec --no-watch --browsers ChromeHeadless --code-coverage",
    "lint": "tsc --noEmit && eslint . --ext .ts --ignore-path .eslintignore --quiet",
    "lintfix": "tsc --noEmit && eslint . --ext .ts --quiet --fix",
    "e2e": "ng e2e",
    "prepare ": "npx husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ./documentation",
    "docs:start": "compodoc -p ./tsconfig.doc.json --theme readthedocs -d ./documentation -n \"PMF Site\"",
    "docs:serve": "npm run docs:start && compodoc -s",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "engines": {
    "node": ">=14.17.1"
  },
  "dependencies": {
    "@angular/animations": "~12.1.5",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "~12.1.5",
    "@angular/compiler": "~12.1.5",
    "@angular/core": "~12.1.5",
    "@angular/forms": "~12.1.5",
    "@angular/platform-browser": "~12.1.5",
    "@angular/platform-browser-dynamic": "~12.1.5",
    "@angular/router": "~12.1.5",
    "bootstrap": "^5.0.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.15.2",
    "primeicons": "^4.1.0",
    "primeng": "^12.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.1.4",
    "@angular/compiler-cli": "~12.1.5",
    "@angular/elements": "^12.2.2",
    "@babel/core": "^7.15.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.4.0-alpha.31",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-essentials": "^6.4.0-alpha.31",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.4.0-alpha.31",
    "@storybook/angular": "^6.4.0-alpha.31",
    "@storybook/builder-webpack5": "^6.4.0-alpha.31",
    "@storybook/manager-webpack5": "^6.4.0-alpha.31",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "@typescript-eslint/typescript-estree": "^4.28.4",
    "@webcomponents/custom-elements": "^1.5.0",
    "angular-oauth2-oidc": "^8.0.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
